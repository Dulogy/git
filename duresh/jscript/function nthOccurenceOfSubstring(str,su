function nthOccurenceOfSubstring(str,subString,n)
{
    let b = str,index;
    let subStringOccurence=[];
    for(let i=0;;){
        index = b.indexOf(subString);
        if(index==-1)break;
        subStringOccurence.push(i+index);
        b=b.slice(index+subString.length);
        i=i+(index+subString.length)
    };
    return subStringOccurence.length<n?false:(subStringOccurence[n-1]+1);
} 
let ans=nthOccurenceOfSubstring("durgeshkumar","u",2);
console.log(ans);